@model BaLogisticsSystem.Models.PersonEntity

@{
    ViewBag.Title = Model.UserName;
}
<div class="row">
    <div class="col-md-10">
        <div class="page-header">
            <h1>Informacija apie <small>@Model.Email</small></h1>
        </div>
    </div>
    <div class="col-md-2">
        <button type="button" class="btn btn-primary  pull-right">Redaguoti vartotoją</button>
    </div>
</div>

<dl class="dl-horizontal">
    <dt>@Html.DisplayNameFor(model => model.Name)</dt>
    <dd>@Html.DisplayFor(model => model.Name)</dd>

    <dt>@Html.DisplayNameFor(model => model.LastName)</dt>
    <dd>@Html.DisplayFor(model => model.LastName)</dd>
    
    <dt>@Html.DisplayNameFor(model => model.UserName)</dt>
    <dd>@Html.DisplayFor(model => model.UserName)</dd>
    
    <dt>@Html.DisplayNameFor(model => model.Email)</dt>
    <dd>@Html.DisplayFor(model => model.Email)</dd>
    
    <dt>@Html.DisplayNameFor(model => model.MobilePhone)</dt>
    <dd>@Html.DisplayFor(model => model.MobilePhone)</dd>
    
    <dt>@Html.DisplayNameFor(model => model.IdOrganization)</dt>
    <dd>@Html.DisplayFor(model => model.IdOrganization)</dd>
</dl>